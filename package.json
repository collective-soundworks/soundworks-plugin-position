{
  "name": "@soundworks/plugin-position",
  "version": "2.0.0-alpha.1",
  "description": "soundworks plugin for locating people in an area",
  "authors": [
    "Benjamin Matuszewski"
  ],
  "license": "BSD-3-Clause",
  "type": "module",
  "exports": {
    "./client.js": {
      "node": "./src/PluginPositionClient.js",
      "default": "./src/PluginPositionClient.browser.js"
    },
    "./server.js": "./src/PluginPositionServer.js"
  },
  "scripts": {
    "api": "jsdoc-to-readme --src src/*.js",
    "lint": "npx eslint src && npx eslint tests",
    "preversion": "npm run api && npm run toc && git commit -am 'docs: build' --allow-empty",
    "test": "mocha tests plugin-position.spec.js",
    "toc": "markdown-toc -i README.md --maxdepth 3"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/collective-soundworks/soundworks-plugin-position"
  },
  "bugs": {
    "url": "https://github.com/collective-soundworks/soundworks-plugin-position/issues"
  },
  "homepage": "https://soundworks.dev",
  "devDependencies": {
    "@ircam/eslint-config": "^1.1.0",
    "@soundworks/core": "^4.0.0-alpha.4",
    "chai": "^4.3.6",
    "jsdoc-to-readme": "^1.1.0",
    "markdown-toc": "^1.2.0",
    "mocha": "^10.1.0"
  },
  "dependencies": {
    "@ircam/sc-components": "^3.0.0-alpha.3",
    "@ircam/sc-utils": "^1.0.1",
    "lit": "^2.7.5"
  }
}
